{
	"name": "web-to-know-website",
	"version": "0.1.0",
	"description": "Web to know website",
	"scripts": {
		"build": "npx @11ty/eleventy",
		"build-ghpages": "npx @11ty/eleventy --pathprefix=/web-to-know-website/",
		"start": "npx @11ty/eleventy --serve --quiet",
		"serve": "npx @11ty/eleventy --serve",
		"debug": "DEBUG=Eleventy* npx @11ty/eleventy",
		"debugstart": "DEBUG=Eleventy* npx @11ty/eleventy --serve --quiet",
		"benchmark": "DEBUG=Eleventy:Benchmark* npx @11ty/eleventy",
		"predeploy": "npm run build-ghpages",
		"deploy": "gh-pages -d _site"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/webtoknow/web-to-know-website.git"
	},
	"author": {
		"name": "Bogdan Nicolae",
		"email": "bmnicolae@gmail.com",
		"url": "https://bogminic.com/"
	},
	"license": "MIT",
	"engines": {
		"node": ">=14"
	},
	"bugs": {
		"url": "https://github.com/webtoknow/web-to-know-website/issues"
	},
	"homepage": "https://webtoknow.github.io/web-to-know-website",
	"devDependencies": {
		"@11ty/eleventy": "^2.0.1",
		"@11ty/eleventy-img": "^3.1.0",
		"@11ty/eleventy-navigation": "^0.3.5",
		"@11ty/eleventy-plugin-bundle": "^1.0.4",
		"@11ty/eleventy-plugin-rss": "^1.2.0",
		"@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
		"eleventy-plugin-i18n": "^0.1.3",
		"eleventy-plugin-nesting-toc": "^1.3.0",
		"eleventy-plugin-time-to-read": "^1.3.0",
		"luxon": "^3.3.0",
		"markdown-it-anchor": "^8.6.7",
		"rosetta": "^1.1.0",
		"gh-pages": "^5.0.0"
	}
}
